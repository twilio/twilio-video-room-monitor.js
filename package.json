{
  "name": "twilio-video-room-monitor",
  "version": "1.0.0",
  "description": "",
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "scripts": {
    "start": "tsc --watch",
    "test": "cross-env TZ=utc jest",
    "test:ci": "cross-env TZ=utc jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage --silent",
    "lint": "eslint src",
    "parcel:watch": "PARCEL_TARGET=browser parcel src/index.tsx --no-hmr --out-dir dist/browser",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "tsc",
    "build:browser": "PARCEL_TARGET=browser parcel build src/index.tsx --no-source-maps --out-dir dist/browser --target browser --experimental-scope-hoisting",
    "ts": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/chrome": "0.0.126",
    "@types/d3-array": "^2.9.0",
    "@types/jest": "26.0.23",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "@visx/xychart": "^1.10.1",
    "cross-env": "7.0.3",
    "d3-array": "^2.12.1",
    "eventemitter3": "^4.0.7",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-spring": "^9.1.2",
    "styled-components": "^5.2.1",
    "twilio-video": "^2.14.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^5.1.2",
    "@types/enzyme": "^3.10.4",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.1",
    "babel-eslint": "10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "7.27.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "ts-jest": "26.5.5",
    "typescript": "4.2.4"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-shadow": "off",
      "@typescript-eslint/no-shadow": [
        "warn"
      ]
    }
  }
}
